nt: 0 },\r\n        { delay: 2.67, count: 0 },\r\n      ],\r\n      activeDropdown: null,\r\n    };\r\n  },\r\n  methods: {\r\n    toggleDropdown(dropdown) {\r\n      this.activeDropdown = this.activeDropdown === dropdown ? null : dropdown;\r\n    },\r\n    incrementCounts() {\r\n      this.dropdowns.forEach((dropdown) => {\r\n        dropdown.count += 1;\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    setInterval(this.incrementCounts, 1330);\r\n  },\r\n  unmounted() {\r\n    clearInterval(this.countIntervals);\r\n  },\r\n};\r\n</script>\r\n\r\n  <style>\r\n  .dropdowns {\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n  \r\n  .dropdown {\r\n    margin: 0 10px;\r\n    list-style: none;\r\n    position: relative;\r\n    animation: count-up 1.33s infinite;\r\n  }\r\n  \r\n  @keyframes count-up {\r\n    from { content: \"\"; }\r\n    to { content: \" . \" counter(count); }\r\n  }\r\n  \r\n  .dropdown-items {\r\n    display: none;\r\n    position: absolute;\r\n    top: 100%;\r\n    left: 0;\r\n    width: 100%;\r\n    padding: 0;\r\n    margin: 0;\r\n    background-color: white;\r\n    border: 1px solid #ccc;\r\n    border-top: none;\r\n    z-index: 1;\r\n  }\r\n  \r\n  .dropdown:hover .dropdown-items {\r\n    display: block;\r\n  }\r\n  \r\n  .dropdown li {\r\n    padding: 10px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .dropdown-items li {\r\n    padding: 10px;\r\n    border-top: 1px solid #ccc;\r\n  }\r\n  \r\n  .dropdown-items li:first-child {\r\n    border-top: none;\r\n  }\r\n  \r\n  .dropdown-items li a {\r\n    display: block;\r\n    background-color: transparent;\r\n    color: black;\r\n    text-decoration: none;\r\n    font-size: 14px;\r\n  }\r\n  \r\n  .dropdown-items li a:hover {\r\n    background-color: #f2f2f2;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAmBA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CACT;QAAEC,KAAK,EAAE,IAAI;QAAEC,KAAK,EAAE;MAAE,CAAC,EACzB;QAAED,KAAK,EAAE,IAAI;QAAEC,KAAK,EAAE;MAAE,CAAC,